<UserControl x:Class="ShadowBBR_Editor.BeatmapLabel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ShadowBBR_Editor"
             mc:Ignorable="d">
	<DockPanel>
		<Rectangle DockPanel.Dock="Bottom" Height="4" Width="1" Fill="#FF404040" HorizontalAlignment="Left" VerticalAlignment="Bottom" />
		<Border Background="White" BorderBrush="DarkGray" BorderThickness="1" HorizontalAlignment="Left" VerticalAlignment="Bottom" Padding="1" DockPanel.Dock="Bottom">
			<TextBlock x:Name="text" FontSize="6" HorizontalAlignment="Left" VerticalAlignment="Top" Text="4" FontFamily="Arial"/>
		</Border>
	</DockPanel>
</UserControl>
