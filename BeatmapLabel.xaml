<UserControl x:Class="ShadowBBR_Editor.BeatmapLabel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ShadowBBR_Editor"
             mc:Ignorable="d">
	<DockPanel>
		<Rectangle DockPanel.Dock="Bottom" Height="3" Width="1" Fill="White" HorizontalAlignment="Left" VerticalAlignment="Bottom" />
		<TextBlock x:Name="text" HorizontalAlignment="Left" VerticalAlignment="Bottom" DockPanel.Dock="Bottom" Text="4" FontFamily="Arial" Foreground="White" Margin="0,1,1,1"/>
	</DockPanel>
</UserControl>
